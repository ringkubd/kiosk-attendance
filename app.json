{
  "expo": {
    "name": "Kiosk Attendance",
    "slug": "kioskattendance",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "kioskattendance",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#CE4631",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kioskattendance.app",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#CE4631",
          "dark": {
            "backgroundColor": "#CE4631"
          }
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your camera for face recognition.",
          "enableMicrophonePermission": false
        }
      ],
      "expo-secure-store",
      "./plugins/withOnnxruntimePackage",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.20",
            "minSdkVersion": 26
          }
        }
      ],
      "expo-asset",
      "expo-sqlite"
    ],
    "extra": {
      "router": {
        "appRoot": "app"
      },
      "eas": {
        "projectId": "b05b6192-ec62-442d-85ed-5aa2e85c4d32"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your camera for face recognition."
      },
      "bundleIdentifier": "com.kioskattendance.app"
    },
    "owner": "ringkubd"
  }
}
